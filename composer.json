{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type":"package",
            "package": {
                "name": "nwidart/laravel-modules",
                "version":"dev",
                "source": {
                    "url": "https://github.com/luongvancong/laravel-modules",
                    "type": "git",
                    "reference":"master"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "zizaco/entrust": "dev-laravel-5",
        "intervention/image": "^2.3@dev",
        "mews/purifier": "~2.0",
        "roumen/sitemap": "2.5.*",
        "laravel/socialite": "^2.0@dev",
        "maatwebsite/excel": "~2.1.0",
        "doctrine/dbal": "2.5.*",
        "illuminate/pagination": "^5.3",
        "buzz/laravel-google-captcha": "^1.3",
        "guzzlehttp/guzzle": "^6.2",
        "swiftmailer/swiftmailer": "^5.4"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "barryvdh/laravel-debugbar": "^2.3",
        "phpspec/phpspec": "~2.1",
        "nwidart/laravel-modules": "@dev"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files" : [
            "app/Hocs/Helpers/helper.php"
        ],
        "psr-4": {
            "Nht\\": "app/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "Nwidart\\Modules\\": "vendor/nwidart/laravel-modules/src"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
